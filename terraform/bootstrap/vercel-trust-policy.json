{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "Federated": "arn:aws:iam::ACCOUNT_ID:oidc-provider/oidc.vercel.com/VERCEL_TEAM_SLUG"
      },
      "Action": "sts:AssumeRoleWithWebIdentity",
      "Condition": {
        "StringEquals": {
          "oidc.vercel.com/VERCEL_TEAM_SLUG:aud": "urn:vercel:VERCEL_TEAM_SLUG"
        }
      }
    }
  ]
}
